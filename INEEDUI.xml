<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

	<Button name="INEEDItemButtonTemplate" virtual="true" inherits="ItemButtonTemplate">
	</Button>
	<FontString name="INEEDQuantityTemplate" virtual="true">
		<Size x="32" y="16"/>
	</FontString>
	<Button name="INEEDLinkTemplate" virtual="true">
		<Size x="100" y="16"/>
	</Button>


	<Frame name="INEEDUIFrame" frameStrata="BACKGROUND" toplevel="false" parent="UIParent" enableMouse="true" movable="false">
		<Size x="384" y="512"/>
		<Anchors>
			<Anchor point="CENTER" relativePoint="CENTER" relativeTo="UIParent"/>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture setAllPoints="true">
					<Color r="0.0" g="0.0" b="0.0"/>
				</Texture>
			</Layer>
			<Layer level="ARTWORK">
				<FontString name="$parent_TitleText" frameStrata="ARTWORK" inherits="GameFontHighlight" text="INEED">
					<Size>
						<AbsDimension x="250" y="14"/>
					</Size>
					<Anchors>
						<Anchor point="TOP" relativePoint="TOP">
							<Offset>
								<AbsDimension x="0" y="-4"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				INEED.UIOnLoad()
				self:Hide()
			</OnLoad>
		</Scripts>
		<!--
		<Frames>
			<Button name="$parent_Item01" inherits="INEEDItemButtonTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$parent"/>
				</Anchors>
			</Button>

		</Frames>
	-->
	</Frame>



</Ui>